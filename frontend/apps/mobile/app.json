{
  "expo": {
    "name": "Seed Hypermedia 2026.1",
    "slug": "seed",
    "scheme": "hm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1F3838"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "media.hyper.seed.gamma",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDisplayName": "Seed 2026",
        "CFBundleName": "Seed Hypermedia Gamma 2026.1"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "media.hyper.seed.gamma"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "89c694bd-957b-40ec-938c-0e963c4094d1"
      }
    }
  }
}
