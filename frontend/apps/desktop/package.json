  "scripts": {
    "dev": "electron-forge start",
    "dev:debug": "ELECTRON_ENABLE_LOGGING=true electron-forge start -- --remote-debugging-port=9222",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "devtools": "react-devtools",
    "test": "NODE_ENV=test yarn package:test && yarn e2e",
    "test:only": "NODE_ENV=test yarn e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "package:test": "NODE_ENV=test VITE_DESKTOP_P2P_PORT=5800 VITE_DESKTOP_HTTP_PORT=58001 VITE_DESKTOP_GRPC_PORT=58002 VITE_DESKTOP_APPDATA=appData.test.local yarn package",
    "e2e": "NODE_ENV=test npx playwright test --project=e2e",
    "e2e:debug": "NODE_ENV=test PWDEBUG=1 yarn e2e",
    "e2e:report": "NODE_ENV=test npx playwright show-report",
    "format:check": "prettier --check . ../../packages",
    "format:write": "prettier --write . ../../packages",
    "test:performance": "ts-node ../performance/run-performance-tests.ts",
    "test:performance-compare": "ts-node ../performance/compare-performance.ts",
    "test:performance:dashboard": "yarn test:performance --dashboard",
    "test:performance:budget": "yarn test:performance --budget",
    "test:performance:trace": "yarn test:performance --trace",
    "test:performance:best-practices": "yarn test:performance --best-practices",
    "test:performance:full": "yarn test:performance --dashboard --budget --trace --best-practices"
  },
