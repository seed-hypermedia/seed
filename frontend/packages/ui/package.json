{
  "name": "@shm/ui",
  "version": "0.1.0",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./types/index",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --skip-types --watch",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "test": "echo NO TESTS",
    "typecheck": "tsc --noEmit",
    "generate": "tamagui generate-themes ./src/themes/theme.ts ./src/themes-generated.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.1",
    "@rehookify/datepicker": "6.6.8",
    "@shm/shared": "workspace:*",
    "@tamagui/animations-moti": "1.108.2",
    "@tamagui/animations-react-native": "1.108.2",
    "@tamagui/button": "1.108.2",
    "@tamagui/checkbox": "1.108.2",
    "@tamagui/cli": "1.108.2",
    "@tamagui/config": "1.108.2",
    "@tamagui/create-theme": "1.108.2",
    "@tamagui/font-inter": "1.108.2",
    "@tamagui/helpers-icon": "1.108.2",
    "@tamagui/input": "1.108.2",
    "@tamagui/lucide-icons": "1.108.2",
    "@tamagui/react-native-media-driver": "1.108.2",
    "@tamagui/shorthands": "1.108.2",
    "@tamagui/stacks": "1.108.2",
    "@tamagui/text": "1.108.2",
    "@tamagui/theme-builder": "1.108.2",
    "@tamagui/themes": "1.108.2",
    "@tamagui/web": "1.108.2",
    "nostr-tools": "1.16.0",
    "react": "18.2.0",
    "react-hook-form": "7.46.1",
    "react-icons": "4.9.0",
    "react-native-reanimated": "3.7.0",
    "react-virtuoso": "4.6.2",
    "tamagui": "1.108.2"
  },
  "devDependencies": {
    "@tamagui/build": "1.108.2",
    "@tamagui/use-did-finish-ssr": "1.100.5"
  }
}
